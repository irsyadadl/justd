{
  "name": "justd",
  "author": "irsyadadl",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "first-time": "bun i && bun run build && next dev",
    "lint": "biome check",
    "build:stubs": "rm -rf public/stubs && bun run resources/scripts/build-stubs.ts",
    "prebuild": "bun run build:registry && bun run build:stubs",
    "cuc": "bun run resources/scripts/check-use-client.ts",
    "cdc": "bun run resources/scripts/check-duplicated-classes.ts",
    "prepare": "husky",
    "build:registry": "rm -rf public/registry && bun run resources/scripts/build-registry.ts",
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "biome lint --fix && biome check --write",
    "format:unsafe": "biome check --unsafe --write",
    "build:everything": "rm -rf pubilc/registry && bun run build:registry && biome format --write __registry__/generated.ts",
    "bb": "bun run format && bun run build:everything",
    "start": "next start",
    "preview": "bun run build && next start",
    "ic": "bun run resources/scripts/identify-color.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@openpanel/nextjs": "^1.0.7",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@shikijs/transformers": "^1.29.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.10.7",
    "babel-plugin-react-compiler": "^19.0.0-beta-e552027-20250112",
    "culori": "^4.0.1",
    "dayjs": "^1.11.13",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "fumadocs-mdx": "^11.3.1",
    "input-otp": "^1.4.2",
    "justd-icons": "^1.10.22",
    "motion": "^11.18.2",
    "next": "^15.1.5",
    "next-themes": "^0.4.4",
    "next-view-transitions": "^0.3.4",
    "ntcjs": "^1.1.3",
    "react": "^19.0.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^19.0.0",
    "react-instantsearch-nextjs": "^0.3.21",
    "react-intersection-observer": "^9.15.1",
    "recharts": "^2.15.0",
    "rehype-stringify": "^10.0.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "sharp": "^0.33.5",
    "shiki": "1.27.2",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "usemods": "^1.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@shikijs/rehype": "^1.29.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/culori": "^2.1.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/title": "^3.4.3",
    "encoding": "^0.1.13",
    "husky": "^9.1.7",
    "postcss": "^8.5.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justdlabs/justd.git"
  },
  "trustedDependencies": ["es5-ext", "esbuild", "sharp"],
  "overrides": {
    "react-is": "^19.0.0"
  },
  "resolutions": {
    "shiki": "1.27.2"
  }
}
