{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string",
      "format": "uri"
    },
    "ui": {
      "type": "string"
    },
    "utils": {
      "type": "string"
    },
    "gray": {
      "type": "string",
      "enum": ["zinc", "gray", "slate", "neutral", "stone"]
    },
    "css": {
      "type": "string"
    },
    "alias": {
      "type": "string"
    },
    "language": {
      "type": "string",
      "enum": ["typescript", "javascript"]
    }
  },
  "required": ["ui", "utils", "gray", "css", "alias"]
}
